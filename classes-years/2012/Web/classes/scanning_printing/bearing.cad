ball5 = sphere(cos(5*2*pi/Nb)*R_r, sin(5*2*pi/Nb)*R_r, 0, b_r)
ball6 = sphere(cos(6*2*pi/Nb)*R_r, sin(6*2*pi/Nb)*R_r, 0, b_r)
balls = add(ball1,ball2)
balls = add(balls,ball3)
balls = add(balls,ball4)
balls = add(balls,ball5)
balls = add(balls,ball6)

bearing = add(outer_part,balls)

# view cutout
bearing = subtract(bearing,cube(0,2,-2,0,-2,2))

